{

    "manifest_version": 2,
    "name": "Wallhaven Tabs",
    "version": "1.0",
  
    "description": "Use wallpapers from wallhaven on start page",
  
    "icons": {
      "48": "icons/border-48.png"
    },

    "applications": {
        "gecko": {
          "id": "wallhaven-start-page@mint.as",
          "strict_min_version": "57.0"
        }
      },


    "permissions": [
        "tabs",
        "storage"
    ],
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "chrome_url_overrides" : {
        "newtab": "app.html"
      }
  
  }